#include<stdio.h>
int main()
{
    int n,greatest;
    printf("Enter the number of elements in array :\n");
    scanf("%d",&n);
    int array[n];
    printf("enter the elements =\n");
    
    for(int i= 0;i<n;i++){
        scanf("%d",&array[i]);
        if(greatest<array[i]){
            greatest = array[i];
        }
    }
    printf("%d",greatest);
    return 0;

}
